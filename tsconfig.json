{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "Ships/*": ["src/model/ships/*"]
    },
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "checkJs": false
  },
  "exclude": ["node_modules", "dist"]
}
